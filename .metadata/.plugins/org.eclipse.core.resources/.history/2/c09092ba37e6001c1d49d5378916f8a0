package Basicbasic;

public class IntitaializationDemo {
int a,b;
{
this.a=2;	
}
public void name(int x) {
	
}
public IntitaializationDemo(int a, int b) {
	this.a = a;
	this.b = b;
}
	public static void main(String[] args) {
		IntitaializationDemo it=new IntitaializationDemo(2, 3);
		it.name(2);
	}
}
